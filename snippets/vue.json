    "dialog alert": {
        "prefix": "b-alert",
        "body": [
            "this.\\$dialog.alert('${text}')"
        ],
        "description": "alert default"
    },
    "dialog alert default": {
        "prefix": "b-alert-custom",
        "body": [
            "this.\\$dialog.alert({",
            "\ttitle: '',",
            "\tmessage: '',",
            "\tconfirmText: '',",
            "})"
        ],
        "description": "Custom alert"
    },
    "dialog confirm": {
        "prefix": "b-confirm",
        "body": [
            "this.\\$dialog.confirm({",
            "\tmessage: '',",
            "\tonConfirm: () => {",
            "\t\tthis.\\$toast.open('Text Confirm')",
            "\t}",
            "})"
        ],
        "description": "Confirm default"
    },
    "dialog confirm custom success": {
        "prefix": "b-confirm-success",
        "body": [
            "this.\\$dialog.confirm({",
            "\ttitle: '',",
            "\tmessage: '',",
            "\tcancelText: 'Disagree',",
            "\tconfirmText: 'Agree',",
            "\ttype: 'is-success',",
            "\tonConfirm: () => {",
            "\t\tthis.\\$toast.open('Text')",
            "\t}",
            "})"
        ],
        "description": "Confirm custom success"
    },
    "dialog confirm custom danger": {
        "prefix": "b-confirm-danger",
        "body": [
            "this.\\$dialog.confirm({",
            "\ttitle: '',",
            "\tmessage: '',",
            "\tconfirmText: '',",
            "\ttype: 'is-danger',",
            "\thasIcon: true,",
            "\tonConfirm: () => {",
            "\t\tthis.\\$toast.open('Text')",
            "\t}",
            "})"
        ],
        "description": "Confirm custom danger"
    },
    "dialog promp": {
        "prefix": "b-prompt",
        "body": [
            "this.\\$dialog.prompt({",
            "\tmessage: '',",
            "\tinputMaxlength: 20,",
            "\tonConfirm: (${1:value}) => {",
            "\t\tthis.\\$toast.open(`\\${${1:value}}`)",
            "\t}",
            "})"
        ],
        "description": "Dialog prompt"
    },
    "toast default": {
        "prefix": "b-toast",
        "body": [
            "this.\\$toast.open('${1:message}')"
        ],
        "description": "Toast are simple text messages to inform the user"
    },
    "toast success": {
        "prefix": "b-toast-success",
        "body": [
            "this.\\$toast.open({",
            "\tmessage: '${1:message}',",
            "\ttype: 'is-success'",
            "})"
        ],
        "description": "Toast are simple text[success] messages to inform the user"
    },
    "toast danger": {
        "prefix": "b-toast-danger",
        "body": [
            "this.\\$toast.open({",
            "\tduration: 5000,",
            "\tmessage: '${1:message}',",
            "\tposition: 'is-bottom',",
            "\ttype: 'is-danger'",
            "})"
        ],
        "description": "Toast are simple text[danger] messages to inform the user"
    }
}